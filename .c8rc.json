{
  "reporter": [
    "text",
    "lcov",
    "json-summary",
    "html"
  ],
  "exclude": [
    "coverage/**",
    "dist/**",
    "tests/**",
    "templates/**",
    "scripts/**",
    "*.config.js",
    "*.config.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "extension": [
    ".ts"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 75,
  "functions": 75,
  "branches": 65,
  "statements": 75
}